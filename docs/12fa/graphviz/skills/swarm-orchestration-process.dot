digraph SWARM_ORCHESTRATION_WORKFLOW {
  // Layout configuration
  rankdir=TB;
  node [shape=box, style="rounded,filled"];

  // =============================================================================
  // INITIALIZATION PHASE
  // =============================================================================
  subgraph cluster_initialization {
    label="Swarm Orchestration Initialization";
    style="filled,rounded";
    color="lightblue";
    bgcolor="#E8F4F8";

    start [label="Orchestration Invoked", shape=ellipse, fillcolor=lightblue];
    load_spec [label="Load Orchestration Plan"];
    validate_input [label="Validate Task Requirements"];
    check_deps [label="Check Agent Dependencies"];

    start -> load_spec;
    load_spec -> validate_input;
    validate_input -> check_deps;
  }

  // =============================================================================
  // VALIDATION & POLICY ENFORCEMENT
  // =============================================================================
  subgraph cluster_validation {
    label="Policy Validation";
    style="filled,rounded";
    color="orange";
    bgcolor="#FFF8E8";

    check_policy [shape=diamond, label="12-FA\nCompliant?"];
    check_secrets [shape=diamond, label="No Secrets\nin Root?"];
    validate_plan [shape=diamond, label="Orchestration\nPlan Valid?"];

    policy_violation [shape=octagon, fillcolor=crimson,
                      label="BLOCKED\nPolicy Violation"];
    secrets_violation [shape=octagon, fillcolor=crimson,
                       label="BLOCKED\nRule #1: No Secrets in Root"];
    plan_error [shape=octagon, fillcolor=orange,
                label="WARN\nInvalid Orchestration Plan"];
  }

  // Connect initialization to validation
  check_deps -> check_policy;
  check_policy -> check_secrets [label="Pass"];
  check_policy -> policy_violation [label="Fail"];
  check_secrets -> validate_plan [label="Pass"];
  check_secrets -> secrets_violation [label="Secrets Found"];
  validate_plan -> plan_error [label="Invalid"];

  // =============================================================================
  // EXECUTION PHASE - TASK ORCHESTRATION
  // =============================================================================
  subgraph cluster_execution {
    label="Task Orchestration Phase";
    style="filled,rounded";
    color="purple";
    bgcolor="#F0E8F8";

    decompose_tasks [label="Decompose Complex Tasks", fillcolor=purple];
    decompose_check [shape=diamond, label="Tasks\nDecomposed?"];

    assign_agents [label="Assign Optimal Agents\nto Tasks", fillcolor=purple];
    assignment_check [shape=diamond, label="Agents\nAssigned?"];

    execute_parallel [label="Execute Tasks in Parallel", fillcolor=purple];
    execution_check [shape=diamond, label="Execution\nSuccessful?"];

    collect_results [label="Collect & Aggregate Results", fillcolor=purple];
    results_check [shape=diamond, label="Results\nComplete?"];

    // Error handling within execution
    decompose_error [shape=octagon, fillcolor=orange,
                     label="Task Decomposition Failed"];
    execution_error [shape=octagon, fillcolor=orange,
                     label="Execution Error"];
    retry_logic [label="Retry Failed Tasks", fillcolor=yellow];
  }

  // Connect validation to execution
  validate_plan -> decompose_tasks [label="Valid"];

  // Execution flow
  decompose_tasks -> decompose_check;
  decompose_check -> assign_agents [label="Yes"];
  decompose_check -> decompose_error [label="No"];

  assign_agents -> assignment_check;
  assignment_check -> execute_parallel [label="Yes"];
  assignment_check -> decompose_error [label="No"];

  execute_parallel -> execution_check;
  execution_check -> collect_results [label="Yes"];
  execution_check -> execution_error [label="No"];

  collect_results -> results_check;
  results_check -> execution_error [label="No"];

  // Error recovery
  decompose_error -> retry_logic;
  execution_error -> retry_logic;
  retry_logic -> assign_agents [label="Retry", style=dashed];
  retry_logic -> escalate [label="Max Retries"];

  // =============================================================================
  // COORDINATION & MEMORY
  // =============================================================================
  subgraph cluster_coordination {
    label="Orchestration Coordination";
    style="filled,rounded";
    color="blue";
    bgcolor="#E8E8F8";

    memory_store [label="Store Orchestration State\nin Memory", fillcolor=lightblue];
    track_progress [label="Track Task Progress", fillcolor=lightblue];
    manage_dependencies [label="Manage Task Dependencies", fillcolor=lightblue];
    check_blocking [shape=diamond, label="Blocking\nDependencies?"];
    notify_coordinator [label="Notify\nCoordinator", fillcolor=lightblue];
  }

  // Connect execution to coordination
  results_check -> memory_store [label="Yes"];
  memory_store -> track_progress;
  track_progress -> manage_dependencies;
  manage_dependencies -> check_blocking;
  check_blocking -> notify_coordinator [label="No"];
  check_blocking -> manage_dependencies [label="Yes", style=dashed];

  // =============================================================================
  // VALIDATION & QUALITY GATES
  // =============================================================================
  subgraph cluster_quality {
    label="Quality Validation";
    style="filled,rounded";
    color="green";
    bgcolor="#E8F8E8";

    validate_output [shape=diamond, label="Output\nComplete?"];
    verify_consistency [label="Verify Result Consistency", fillcolor=lightgreen];
    consistency_check [shape=diamond, label="Consistent?"];

    run_tests [label="Run Integration Tests", fillcolor=lightgreen];
    test_results [shape=diamond, label="Tests Pass?"];

    quality_gate_fail [shape=octagon, fillcolor=orange,
                       label="Quality Gate Failed"];
  }

  // Connect coordination to quality
  notify_coordinator -> validate_output;

  // Quality flow
  validate_output -> verify_consistency [label="Yes"];
  validate_output -> quality_gate_fail [label="No"];

  verify_consistency -> consistency_check;
  consistency_check -> run_tests [label="Yes"];
  consistency_check -> quality_gate_fail [label="No"];

  run_tests -> test_results;
  test_results -> quality_gate_fail [label="Fail"];

  // =============================================================================
  // COMPLETION & CLEANUP
  // =============================================================================
  subgraph cluster_completion {
    label="Completion";
    style="filled,rounded";
    color="green";
    bgcolor="#E8F8E8";

    finalize_results [label="Finalize Aggregated Results", fillcolor=lightgreen];
    cleanup [label="Cleanup Agent Resources", fillcolor=lightgreen];
    log_completion [label="Log Orchestration Metrics"];
    success [label="Orchestration Complete", shape=doublecircle,
             fillcolor=green, style="filled,bold"];
  }

  // Connect quality to completion
  test_results -> finalize_results [label="Pass"];
  finalize_results -> cleanup;
  cleanup -> log_completion;
  log_completion -> success;

  // =============================================================================
  // ERROR ESCALATION PATH
  // =============================================================================
  subgraph cluster_errors {
    label="Error Handling & Escalation";
    style="filled,rounded";
    color="red";
    bgcolor="#F8E8E8";

    escalate [shape=hexagon, fillcolor=yellow,
              label="Escalate to User"];
    log_error [label="Log Error Details", fillcolor=orange];
    rollback [label="Rollback Changes", fillcolor=orange];
    failure [label="Orchestration Failed", shape=doublecircle,
             fillcolor=red, style="filled,bold"];
  }

  // Error escalation flow
  escalate -> log_error;
  quality_gate_fail -> log_error;
  policy_violation -> log_error;
  secrets_violation -> log_error;
  plan_error -> log_error;

  log_error -> rollback;
  rollback -> failure;

  // =============================================================================
  // LEGEND
  // =============================================================================
  subgraph cluster_legend {
    label="Legend";
    rank=sink;
    style="filled,rounded";
    bgcolor="#F8F8F8";

    legend_start [shape=ellipse, label="Entry Point", fillcolor=lightblue];
    legend_decision [shape=diamond, label="Decision"];
    legend_process [shape=box, label="Process", fillcolor=purple];
    legend_block [shape=octagon, fillcolor=crimson, label="Blocker"];
    legend_warn [shape=octagon, fillcolor=orange, label="Warning"];
    legend_manual [shape=hexagon, fillcolor=yellow, label="Manual"];
    legend_success [shape=doublecircle, fillcolor=green, label="Success"];
    legend_failure [shape=doublecircle, fillcolor=red, label="Failure"];

    // Hide from main layout
    legend_start -> legend_decision [style=invis];
    legend_decision -> legend_process [style=invis];
    legend_process -> legend_block [style=invis];
    legend_block -> legend_warn [style=invis];
    legend_warn -> legend_manual [style=invis];
    legend_manual -> legend_success [style=invis];
    legend_success -> legend_failure [style=invis];
  }
}
