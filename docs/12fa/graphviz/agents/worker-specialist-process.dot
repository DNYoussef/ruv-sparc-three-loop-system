// Worker Specialist Agent - Specialized Task Execution
// Specialized execution, efficiency, coordination, task completion
digraph worker_specialist_agent {
    graph [rankdir=TB, bgcolor="#0a0e1a", fontname="Arial", fontsize=12, splines=ortho, nodesep=0.6, ranksep=1.0];
    node [shape=box, style="rounded,filled", fontname="Arial", fontsize=10, margin=0.2];
    edge [fontname="Arial", fontsize=9, color="#4a90e2"];

    subgraph cluster_init {
        label="Agent Initialization"; style=filled; color="#1a1f35"; fontcolor="#ffffff";
        worker_init [label="Worker Specialist\nInitialize", fillcolor="#d97706", fontcolor="#ffffff"];
        specialization_config [label="Specialization\nConfig", fillcolor="#2d3748", fontcolor="#ffffff"];
        tool_library [label="Tool\nLibrary", fillcolor="#2d3748", fontcolor="#ffffff"];
    }

    subgraph cluster_task {
        label="Task Reception"; style=filled; color="#1a1f35"; fontcolor="#ffffff";
        task_receiver [label="Task\nReceiver", fillcolor="#2563eb", fontcolor="#ffffff"];
        task_parser [label="Task\nParser", fillcolor="#2563eb", fontcolor="#ffffff"];
        requirement_analyzer [label="Requirement\nAnalyzer", fillcolor="#2563eb", fontcolor="#ffffff"];
        resource_checker [label="Resource\nChecker", fillcolor="#2563eb", fontcolor="#ffffff"];
    }

    subgraph cluster_planning {
        label="Execution Planning"; style=filled; color="#1a1f35"; fontcolor="#ffffff";
        approach_designer [label="Approach\nDesigner", fillcolor="#059669", fontcolor="#ffffff"];
        tool_selector [label="Tool\nSelector", fillcolor="#059669", fontcolor="#ffffff"];
        timeline_estimator [label="Timeline\nEstimator", fillcolor="#059669", fontcolor="#ffffff"];
    }

    subgraph cluster_execution {
        label="Specialized Execution"; style=filled; color="#1a1f35"; fontcolor="#ffffff";
        task_executor [label="Task\nExecutor", fillcolor="#0891b2", fontcolor="#ffffff"];
        quality_monitor [label="Quality\nMonitor", fillcolor="#0891b2", fontcolor="#ffffff"];
        progress_tracker [label="Progress\nTracker", fillcolor="#0891b2", fontcolor="#ffffff"];
        error_handler [label="Error\nHandler", fillcolor="#0891b2", fontcolor="#ffffff"];
    }

    subgraph cluster_optimization {
        label="Efficiency Optimization"; style=filled; color="#1a1f35"; fontcolor="#ffffff";
        performance_optimizer [label="Performance\nOptimizer", fillcolor="#dc2626", fontcolor="#ffffff"];
        resource_optimizer [label="Resource\nOptimizer", fillcolor="#dc2626", fontcolor="#ffffff"];
        workflow_improver [label="Workflow\nImprover", fillcolor="#dc2626", fontcolor="#ffffff"];
    }

    subgraph cluster_validation {
        label="Validation"; style=filled; color="#1a1f35"; fontcolor="#ffffff";
        output_validator [label="Output\nValidator", fillcolor="#8b4789", fontcolor="#ffffff"];
        quality_checker [label="Quality\nChecker", fillcolor="#8b4789", fontcolor="#ffffff"];
        completeness_verifier [label="Completeness\nVerifier", fillcolor="#8b4789", fontcolor="#ffffff"];
    }

    subgraph cluster_reporting {
        label="Reporting"; style=filled; color="#1a1f35"; fontcolor="#ffffff";
        result_packager [label="Result\nPackager", fillcolor="#ef4444", fontcolor="#ffffff"];
        metric_reporter [label="Metric\nReporter", fillcolor="#ef4444", fontcolor="#ffffff"];
        status_updater [label="Status\nUpdater", fillcolor="#ef4444", fontcolor="#ffffff"];
    }

    subgraph cluster_collab {
        label="Agent Collaboration"; style=filled; color="#1a1f35"; fontcolor="#ffffff";
        queen_coord [label="Queen Seraphina\nCoordination", fillcolor="#6366f1", fontcolor="#ffffff"];
        scout_coord [label="Scout Explorer\nCoordination", fillcolor="#6366f1", fontcolor="#ffffff"];
    }

    worker_init -> specialization_config [label="Load"];
    worker_init -> tool_library [label="Load"];
    specialization_config -> task_receiver [label="Receive Task"];
    task_receiver -> task_parser [label="Parse"];
    task_parser -> requirement_analyzer [label="Analyze"];
    requirement_analyzer -> resource_checker [label="Check Resources"];
    resource_checker -> approach_designer [label="Design Approach"];
    approach_designer -> tool_selector [label="Select Tools"];
    tool_selector -> timeline_estimator [label="Estimate Timeline"];
    timeline_estimator -> task_executor [label="Execute"];
    task_executor -> quality_monitor [label="Monitor Quality"];
    quality_monitor -> progress_tracker [label="Track Progress"];
    progress_tracker -> error_handler [label="Handle Errors"];
    error_handler -> performance_optimizer [label="Optimize"];
    performance_optimizer -> resource_optimizer [label="Optimize Resources"];
    resource_optimizer -> workflow_improver [label="Improve Workflow"];
    workflow_improver -> output_validator [label="Validate Output"];
    output_validator -> quality_checker [label="Check Quality"];
    quality_checker -> completeness_verifier [label="Verify Completeness"];
    completeness_verifier -> result_packager [label="Package Results"];
    result_packager -> metric_reporter [label="Report Metrics"];
    metric_reporter -> status_updater [label="Update Status"];
    status_updater -> queen_coord [label="Report to Queen"];
    task_receiver -> scout_coord [label="Request Exploration"];

    subgraph cluster_legend {
        label="Legend"; style=filled; color="#1a1f35"; fontcolor="#ffffff";
        legend1 [label="Worker Specialist", fillcolor="#d97706", fontcolor="#ffffff"];
        legend2 [label="Task Reception", fillcolor="#2563eb", fontcolor="#ffffff"];
        legend3 [label="Execution", fillcolor="#0891b2", fontcolor="#ffffff"];
    }
}
