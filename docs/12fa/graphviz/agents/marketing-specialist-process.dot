digraph marketing_specialist_workflow {
    // Graph properties
    rankdir=TB;
    bgcolor="#f8f9fa";
    fontname="Arial";
    fontsize=14;
    label="Marketing Specialist Agent Workflow\nAgent ID: agent-078 | Category: Business\nPurpose: Digital marketing and campaign management";
    labelloc=t;
    compound=true;

    // Node styling
    node [fontname="Arial", fontsize=11, style="filled"];

    // Start/End nodes
    start [label="Campaign Request", shape=oval, fillcolor="#e8f5e9", color="#4caf50", penwidth=2];
    end [label="Campaign\nLaunched", shape=oval, fillcolor="#e8f5e9", color="#4caf50", penwidth=2];

    // Phase 1: Strategy Development
    subgraph cluster_strategy {
        label="Phase 1: Strategy Development";
        style=filled;
        fillcolor="#fff3e0";
        color="#ff9800";
        penwidth=2;

        s1 [label="Market Research", shape=box, fillcolor="#ffecb3"];
        s2 [label="Audience Targeting", shape=box, fillcolor="#ffecb3"];
        s3 [label="Competitor Analysis", shape=box, fillcolor="#ffecb3"];
        s4 [label="Campaign Goals\n(KPIs)", shape=box, fillcolor="#ffecb3"];
        s5 [label="Budget Allocation", shape=box, fillcolor="#ffecb3"];

        s1 -> s2 -> s3 -> s4 -> s5;
    }

    // Phase 2: Content Creation
    subgraph cluster_content {
        label="Phase 2: Content Creation";
        style=filled;
        fillcolor="#e3f2fd";
        color="#2196f3";
        penwidth=2;

        c1 [label="Campaign\nMessaging", shape=box, fillcolor="#bbdefb"];
        c2 [label="Visual Assets", shape=box, fillcolor="#bbdefb"];
        c3 [label="Copy Writing", shape=box, fillcolor="#bbdefb"];
        c4 [label="Landing Pages", shape=box, fillcolor="#bbdefb"];
        c5 [label="Call-to-Action\nDesign", shape=box, fillcolor="#bbdefb"];

        c1 -> c2 -> c3 -> c4 -> c5;
    }

    // Phase 3: Social Media Marketing
    subgraph cluster_social {
        label="Phase 3: Social Media Marketing";
        style=filled;
        fillcolor="#f3e5f5";
        color="#9c27b0";
        penwidth=2;

        sm1 [label="Platform Selection", shape=box, fillcolor="#e1bee7"];
        sm2 [label="Content Calendar", shape=box, fillcolor="#e1bee7"];
        sm3 [label="Post Scheduling", shape=box, fillcolor="#e1bee7"];
        sm4 [label="Community\nEngagement", shape=box, fillcolor="#e1bee7"];
        sm5 [label="Influencer\nOutreach", shape=box, fillcolor="#e1bee7"];

        sm1 -> sm2 -> sm3 -> sm4 -> sm5;
    }

    // Phase 4: Email Marketing
    subgraph cluster_email {
        label="Phase 4: Email Marketing";
        style=filled;
        fillcolor="#fce4ec";
        color="#e91e63";
        penwidth=2;

        em1 [label="List Segmentation", shape=box, fillcolor="#f8bbd0"];
        em2 [label="Email Template\nDesign", shape=box, fillcolor="#f8bbd0"];
        em3 [label="A/B Testing", shape=box, fillcolor="#f8bbd0"];
        em4 [label="Automation Setup", shape=box, fillcolor="#f8bbd0"];
        em5 [label="Drip Campaigns", shape=box, fillcolor="#f8bbd0"];

        em1 -> em2 -> em3 -> em4 -> em5;
    }

    // Phase 5: Paid Advertising
    subgraph cluster_paid {
        label="Phase 5: Paid Advertising";
        style=filled;
        fillcolor="#e0f2f1";
        color="#009688";
        penwidth=2;

        pa1 [label="Ad Platform\nSelection", shape=box, fillcolor="#b2dfdb"];
        pa2 [label="Ad Creative\nDevelopment", shape=box, fillcolor="#b2dfdb"];
        pa3 [label="Targeting Setup", shape=box, fillcolor="#b2dfdb"];
        pa4 [label="Bid Management", shape=box, fillcolor="#b2dfdb"];
        pa5 [label="Retargeting\nCampaigns", shape=box, fillcolor="#b2dfdb"];

        pa1 -> pa2 -> pa3 -> pa4 -> pa5;
    }

    // Phase 6: Analytics & Optimization
    subgraph cluster_analytics {
        label="Phase 6: Analytics & Optimization";
        style=filled;
        fillcolor="#fff9c4";
        color="#fbc02d";
        penwidth=2;

        a1 [label="Campaign Tracking", shape=box, fillcolor="#fff59d"];
        a2 [label="Conversion Analysis", shape=box, fillcolor="#fff59d"];
        a3 [label="ROI Calculation", shape=box, fillcolor="#fff59d"];
        a4 [label="Performance\nReporting", shape=box, fillcolor="#fff59d"];
        a5 [label="Continuous\nOptimization", shape=box, fillcolor="#fff59d"];

        a1 -> a2 -> a3 -> a4 -> a5;
    }

    // Decision nodes
    approval [label="Campaign\nApproved?", shape=diamond, fillcolor="#ffeb3b", color="#f57f17", penwidth=2];
    performance [label="Meets KPIs?", shape=diamond, fillcolor="#ffeb3b", color="#f57f17", penwidth=2];

    // Parallel execution marker
    parallel [label="Execute Parallel\nChannels", shape=parallelogram, fillcolor="#c8e6c9", color="#388e3c", penwidth=2];

    // Flow connections
    start -> s1 [label="Begin Planning"];
    s5 -> c1 [label="Strategy\nApproved"];
    c5 -> approval [label="Review\nMaterials"];

    approval -> parallel [label="Yes"];
    approval -> c1 [label="No - Revise", style=dashed, color="#f44336"];

    parallel -> sm1 [label="Social Media"];
    parallel -> em1 [label="Email"];
    parallel -> pa1 [label="Paid Ads"];

    sm5 -> a1;
    em5 -> a1;
    pa5 -> a1;

    a5 -> performance [label="Analyze\nResults"];
    performance -> end [label="Yes - Success"];
    performance -> a5 [label="No - Optimize", style=dashed, color="#ff9800"];

    // Capabilities legend
    capabilities [label="Key Capabilities:\n• Campaign Management\n• Social Media Marketing\n• Email Marketing\n• Paid Advertising\n• Content Strategy\n• Analytics & Reporting", shape=note, fillcolor="#e8eaf6", color="#3f51b5", fontsize=10];

    // Tools & Platforms
    tools [label="Platforms:\n• Google Ads\n• Facebook Ads Manager\n• Mailchimp/HubSpot\n• Hootsuite/Buffer\n• Google Analytics", shape=note, fillcolor="#ede7f6", color="#673ab7", fontsize=10];

    // Metadata
    metadata [label="Agent: marketing-specialist\nPriority: Medium\nComplexity: Medium\nGraphviz Priority: 78", shape=note, fillcolor="#f5f5f5", color="#9e9e9e", fontsize=9];
}
